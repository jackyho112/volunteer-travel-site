{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint js/**/*.js js/**/*.jsx webpack.config.js --fix",
    "pro-build": "NODE_ENV=production webpack -p",
    "dev-build": "webpack",
    "dev": "webpack-dev-server --open",
    "watch": "npm run dev-build -- --watch",
    "test": "NODE_ENV=test jest",
    "update-test": "npm run test -- -u",
    "test-coverage": "npm run test -- --coverage",
    "test-coverage-html": "open coverage/lcov-report/index.html",
    "server": "NODE_ENV=server nodemon server.js"
  },
  "dependencies": {
    "express": "4.15.0",
    "history": "4.5.1",
    "lodash": "4.17.4",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router": "4.0.0",
    "react-router-dom": "4.0.0",
    "material-design-lite": "1.3.0",
    "masonry-layout": "4.2.0"
  },
  "devDependencies": {
    "babel-core": "6.16.0",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2017": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.16.0",
    "css-loader": "0.25.0",
    "enzyme": "2.7.1",
    "enzyme-to-json": "^1.3.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "1.6.3",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-react": "6.10.0",
    "jest": "^18.1.0",
    "jsdom": "9.5.0",
    "node-sass": "4.5.0",
    "react-addons-test-utils": "15.3.2",
    "react-test-renderer": "15.3.2",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "prop-types": "^15.5.8",
    "file-loader": "0.11.1"
  },
  "directories": {
    "test": "test"
